{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".svelte-kit/**",
        "!.svelte-kit/types/**",
        "build/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check": {},
    "lint": {},
    "test": {},
    "clean": {
      "cache": false
    }
  }
}
